{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "java:visual-testing",
      "request": "launch",
      "mainClass": "VisualTesting",
      "sourcePath": [
        "${workspaceRoot}/java/visual-testing/src/main/java"
      ],
      "args": ""
    },
    {
      "name": "mocha:visual-testing",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/javascript/visual-testing/node_modules/mocha/bin/_mocha",
      "args": [
        "-u",
        "tdd",
        "--timeout",
        "999999",
        "--colors",
        "${workspaceRoot}/javascript/visual-testing/test/**/*.test.js"
      ],
      "internalConsoleOptions": "openOnSessionStart",
      "console": "integratedTerminal"
    },
    {
      "name": "mocha:hiding-images",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/javascript/hiding-images/node_modules/mocha/bin/_mocha",
      "args": [
        "-u",
        "tdd",
        "--timeout",
        "999999",
        "--colors",
        "${workspaceRoot}/javascript/hiding-images/test/**/*.test.js"
      ],
      "internalConsoleOptions": "openOnSessionStart",
      "env": {
        "APPLITOOLS_APIKEY": "sdfasdff"
      },
      "console": "integratedTerminal"
    },
    {
      "name": "mocha:node",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/javascript/node/node_modules/mocha/bin/_mocha",
      "args": [
        "-u",
        "tdd",
        "--timeout",
        "999999",
        "--colors",
        "${workspaceRoot}/javascript/node/test/**/*.test.js"
      ],
      "internalConsoleOptions": "openOnSessionStart",
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${file}"
    }
  ]
}